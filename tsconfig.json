{
    "compilerOptions": {
      "target": "es2018",
      "module": "CommonJS",
      "lib": ["es2018", "esnext.asynciterable"],
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "sourceMap": true,
      "outDir": "./dist",
      "moduleResolution": "node",
      "baseUrl": ".",
      "rootDir": "src",
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "esModuleInterop":true,
    },
    "include": ["./src/**/*.tsc", "./src/**/*.ts"],
    "exclude": ["node_modules"]
  }